{
    "function": {
        "prefix": "function",
        "body": [
            "function! ${1:name}($2) abort",
            "\t$0",
            "endfunction"
        ]
    },
    "if": {
        "prefix": "if",
        "body": [
            "if ${1:expr1}",
            "\t$0",
            "endif"
        ]
    },
    "for": {
        "prefix": "for",
        "body": [
            "for ${1:var} in ${2:object}",
            "\t$0",
            "endfor"
        ]
    },
    "while": {
        "prefix": "while",
        "body": [
            "while ${1:expr1}",
            "\t$0",
            "endwhile"
        ]
    },
    "augroup": {
        "prefix": "augroup",
        "body": [
            "augroup ${1:name}",
            "\tautocmd!",
            "\t$0",
            "augroup END"
        ]
    },
    "autocmd": {
        "prefix": "autocmd",
        "body": [
            "autocmd ${1:event} ${2:pat} $0"
        ]
    },
    "try": {
        "prefix": "try",
        "body": [
            "try",
            "\t$1",
            "catch /${2:pattern}/",
            "\t$0",
            "endtry"
        ]
    }
}
